DROP TABLE STUD;

CREATE TABLE STUD (
    ID INT PRIMARY KEY,
    NAME VARCHAR2(20)
);

INSERT INTO STUD VALUES(1, 'ABC');
INSERT INTO STUD VALUES(2, 'ABC');
INSERT INTO STUD VALUES(3, 'DEF');
INSERT INTO STUD VALUES(4, 'GHI');

SELECT DISTINCT ID, NAME FROM STUD; -- Fetches all records since we are asking for distinct pairs.